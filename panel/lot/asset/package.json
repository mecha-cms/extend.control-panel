{
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-private-methods": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@taufik-nurrohman/event": "^0.0.4",
    "@taufik-nurrohman/f3h": "^1.0.18",
    "@taufik-nurrohman/hook": "^0.0.2",
    "@taufik-nurrohman/is": "^0.0.5",
    "@taufik-nurrohman/pattern": "^0.0.5",
    "@taufik-nurrohman/tag-picker": "^3.0.14",
    "@taufik-nurrohman/text-editor": "^3.1.10",
    "clean-css-cli": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.34.2",
    "sass": "^1.29.0",
    "terser": "^5.5.0"
  },
  "scripts": {
    "build": "npm-run-all --parallel css js",
    "css": "npm-run-all css:build css:minify",
    "css:build": "sass --style expanded --no-error-css --no-source-map scss/panel.scss:css/panel.css",
    "css:minify": "cleancss -02 --output css/panel.min.css css/panel.css",
    "js": "npm-run-all js:build js:minify",
    "js:build": "rollup --config package.js",
    "js:minify": "terser js/panel.js --mangle --output js/panel.min.js"
  }
}
