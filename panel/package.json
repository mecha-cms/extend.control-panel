{
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-private-methods": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@taufik-nurrohman/event": "^0.0.4",
    "@taufik-nurrohman/f3h": "^1.0.18",
    "@taufik-nurrohman/hook": "^0.0.2",
    "@taufik-nurrohman/is": "^0.0.5",
    "@taufik-nurrohman/pattern": "^0.0.5",
    "@taufik-nurrohman/tag-picker": "^3.0.14",
    "@taufik-nurrohman/text-editor": "^3.1.10",
    "clean-css-cli": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.34.2",
    "sass": "^1.29.0",
    "terser": "^5.5.0"
  },
  "scripts": {
    "css": "npm-run-all css:pack css:minify",
    "css:minify": "cleancss --level 2 --output lot/asset/css/panel.min.css lot/asset/css/panel.css && cleancss --level 2 --output lot/layout/asset/css/minima.min.css lot/layout/asset/css/minima.css",
    "css:pack": "sass --style expanded --no-charset --no-error-css --no-source-map lot/asset/scss/panel.scss:lot/asset/css/panel.css lot/layout/asset/scss/minima.scss:lot/layout/asset/css/minima.css",
    "js": "npm-run-all js:pack js:minify",
    "js:minify": "terser lot/asset/js/panel.js --mangle --output lot/asset/js/panel.min.js",
    "js:pack": "rollup --config package.js",
    "pack": "npm-run-all --parallel css js"
  }
}
